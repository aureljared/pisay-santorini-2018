<Player>:
	canvas.before:
		PushMatrix
		Rotate:
			angle: 352.2
			origin: self.pos
	canvas.after:
		PopMatrix
	canvas:
		Color:
			rgba: 0.25 + 0.5*float(self.num),0.25,0.75-0.5*float(self.num), self.isCurrent
		Rectangle:
			pos: self.pos
			size: self.size
		Color:
			rgba:5,5,5,self.isCurrent
		Rectangle:
